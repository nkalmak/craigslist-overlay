{
	"manifest_version":2,
	"name": "Craigslist Overlay",
	"description": "Display comments on Craigslist.",
	"version": "1",
	
	"permissions": ["activeTab"
		],
		
	"icons": {
    "16": "assets/search.png"
	},
	"content_scripts": [
		{
		  "matches": ["https://*.craigslist.org/*"],
		  "css": ["myStyles.css"],
		  "js": ["contentScript.js","ericapi.js"]
		}
	  ]
}